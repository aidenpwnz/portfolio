{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@aidenpwnz/web/*": [
        "./apps/web/*"
      ],
      "@aidenpwnz/utils": [
        "./libs/utils"
      ],
      "@aidenpwnz/components": [
        "./libs/ui/components"
      ],
      "@aidenpwnz/assets/*": [
        "./libs/ui/assets/*"
      ]
    }
  }
}
